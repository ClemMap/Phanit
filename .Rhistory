library("jsonlite")
get_sncf <- function(dataset = "ponctualite mensuelle transilien") {
cli <- HttpClient$new(
url = "https://ressources.data.sncf.com/api/records/1.0/search/"
)
res <- cli$get(query = list(rows = -1, dataset = dataset))
res$raise_for_statut()
txt <- res$parse("UTF-8")
json <- fromJSON(txt)
return(json$records)
}
